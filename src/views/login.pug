extends base

block content 
    if errorMessage 
        span=errorMessage
    form(method="POST")
        input(placeholder="userName", name="userName", type="text", required)
        input(placeholder="password", name="password", type="password", required)
        input(type="submit", value="login")
        br
        a(href="/users/github/start") github login
    hr
    div 
        span Does not have an account?
        a(href="/join") Create an account